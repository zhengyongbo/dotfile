{
    "vim.leader": ";",
    "workbench.iconTheme": "vs-minimal",
    "editor.tabSize": 2,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "line",
    "terminal.external.osxExec": "iTerm.app",
    "vim.useSystemClipboard": false,
    "editor.tabCompletion": "on",
    "search.exclude": {
        "**/.DS_Store": true,
        "**/.sass-cache": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/temp": true,
        "**/tmp": true,
        "**/vendor": true
    },
    "editor.fontFamily": "Fira Code,Source Code Pro, Menlo, Monaco,'Courier New', monospace",
    "editor.lineHeight": 23,
    "editor.fontLigatures": true,
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "workbench.colorTheme": "Monokai",
    "view-in-browser.customBrowser": "chrome",
    "window.zoomLevel": 0,
    "material-icon-theme.showUpdateMessage": false,
    "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
    "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.US",
    "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
    "vim.autoSwitchInputMethod.enable": true,
    "editor.minimap.maxColumn": 100,
    "clock.dateFormat": "HH:MM ddd mm-dd",
    "window.titleBarStyle": "native",
    "editor.fontSize": 16,
    "workbench.fontAliasing": "auto",
    "less.lint.fontFaceProperties": "ignore",
    "markdown.preview.fontFamily": "Fira Code,-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "Fira Code,Source Code Pro, Menlo, Monaco,'Courier New', monospace",
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.fontWeight": "400",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.fontWeight": "400",
    "terminal.integrated.fontWeightBold": "400",
    "breadcrumbs.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "fileheader.Author": "zhengyongbo",
    "fileheader.LastModifiedBy": "zhengyongbo",
    "docthis.authorName": "zhengyongbo",
    "docthis.includeAuthorTag": true,
    "docthis.includeDateTag": true,
    "docthis.includeDescriptionTag": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#6d4c41 ",
    "vim.easymotionMarkerForegroundColorOneChar": "#fff",
    "vim.easymotionMarkerWidthPerChar": 14,
    "vim.easymotionMarkerHeight": 23,
    "vim.easymotionMarkerForegroundColorTwoChar": "#fff",
    "vim.easymotionMarkerFontFamily": "Fira Code,Source Code Pro, Menlo, Monaco,'Courier New', monospace",
    "vim.easymotionMarkerFontSize": "15",
    "files.exclude": {
        "**/.sass-cache": true
    },
    "files.watcherExclude": {
        "**/.sass-cache/**": true,
        "**/.svn/**": true
    }
}
